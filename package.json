{
  "name": "fullstack",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev -p 3000",
    "prod": "pm2 start npm -- run start",
    "build": "next build",
    "start": "next start -p 3000",
    "pm2": "pm2",
    "pm2:list": "pm2 list",
    "prettier": "npx prettier -w .",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "prisma": "npx prisma",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate",
    "db:dev": "dotenv -e .env.development -- npx prisma studio",
    "db:prod": "dotenv -e .env.production -- npx prisma studio",
    "dco:up": "docker-compose up --build",
    "dco:down": "docker-compose down"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.8.1",
    "@storyofams/next-api-decorators": "^1.7.1",
    "axios": "^0.25.0",
    "babel-plugin-styled-components": "^2.0.2",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^4.1.1",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "next": "12.0.9",
    "next-auth": "^4.1.2",
    "pm2": "^5.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^5.1.0",
    "styled-components": "^5.3.3",
    "swr": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@next/bundle-analyzer": "^12.0.9",
    "@types/axios": "^0.14.0",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cypress": "^9.3.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.5.5"
  }
}
